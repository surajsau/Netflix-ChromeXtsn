{
  "manifest_version": 2,
 
  "name": "WatchedFlix",
  "version": "0.0.1",
  "description": "Easily maintain your 'watched' list Netflix shows or movies",
 
  "content_scripts": [{
    "js": [
    	"lib/underscore-min.js",
      "lib/rate_limit.js",
      "lib/jquery-3.5.0.min.js",
      "lib/jquery.visible.min.js",
      "lib/jquery-observe.js",
    	"js/script.js",
      "js/ui.js",
      "js/data.js",
      "js/db.js"
    ],
    "css": [
    	"css/style.css"
    ],
    "matches": ["*://www.netflix.com/*"]
  }],
  "web_accessible_resources":[
    "watched_list/*",
    "js/db.js"
  ],
  "icons":{
    "16":"icons/icon16.png",
    "48":"icons/icon48.png",
    "128":"icons/icon128.png"
  },

  "permissions": ["storage", "unlimitedStorage"]
 
}